:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x556542ff03b0 .scope module, "licznik_tb" "licznik_tb" 2 1;
 .timescale 0 0;
v0x556543010af0_0 .var "Q", 3 0;
v0x556543010bd0_0 .var/2u *"_s0", 0 0; Local signal
v0x556543010c90_0 .var/2u *"_s1", 0 0; Local signal
v0x556543010d80_0 .net "cnt", 3 0, v0x556543010960_0;  1 drivers
v0x556543010e70_0 .var "load", 0 0;
v0x556543010f60_0 .var "sys_clk", 0 0;
S_0x556542ff0530 .scope module, "UUT" "licznik" 2 8, 3 1 0, S_0x556542ff03b0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk"
    .port_info 1 /INPUT 1 "load"
    .port_info 2 /INPUT 4 "i_Q"
    .port_info 3 /OUTPUT 4 "o_Q"
v0x556542ff0770_0 .net "clk", 0 0, v0x556543010f60_0;  1 drivers
v0x5565430107b0_0 .net "i_Q", 3 0, v0x556543010af0_0;  1 drivers
v0x556543010890_0 .net "load", 0 0, v0x556543010e70_0;  1 drivers
v0x556543010960_0 .var "o_Q", 3 0;
E_0x556542fb48a0 .event posedge, v0x556543010890_0, v0x556542ff0770_0;
    .scope S_0x556542ff0530;
T_0 ;
    %wait E_0x556542fb48a0;
    %load/vec4 v0x556543010890_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %load/vec4 v0x5565430107b0_0;
    %assign/vec4 v0x556543010960_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x556542ff0770_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %load/vec4 v0x556543010960_0;
    %addi 1, 0, 4;
    %assign/vec4 v0x556543010960_0, 0;
T_0.2 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x556542ff03b0;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x556543010f60_0, 0, 1;
    %pushi/vec4 6, 0, 4;
    %store/vec4 v0x556543010af0_0, 0, 4;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x556543010e70_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x556543010bd0_0, 0, 1;
    %pushi/vec4 100, 0, 64;
    %ix/vec4 4;
    %delayx 4;
    %load/vec4 v0x556543010bd0_0;
    %store/vec4 v0x556543010e70_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x556543010c90_0, 0, 1;
    %pushi/vec4 100, 0, 64;
    %ix/vec4 4;
    %delayx 4;
    %load/vec4 v0x556543010c90_0;
    %store/vec4 v0x556543010e70_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x556542ff03b0;
T_2 ;
    %vpi_call 2 21 "$monitor", $time, " | %d", v0x556543010d80_0 {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x556542ff03b0;
T_3 ;
    %delay 25, 0;
    %load/vec4 v0x556543010f60_0;
    %inv;
    %pushi/vec4 1, 0, 1;
    %add;
    %assign/vec4 v0x556543010f60_0, 0;
    %jmp T_3;
    .thread T_3;
